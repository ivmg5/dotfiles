# vi: ft=dosini
[main]
smart_completion = True
multi_line = True
destructive_warning = True
log_file = ~/.mycli.log
log_level = INFO
timing = False
beep_after_seconds = 0
table_format = fancy_grid
redirect_format = csv
post_redirect_command =
syntax_style = monokai
key_bindings = emacs
wider_completion_menu = False
prompt = '\x1b[1;32m\t\x1b[0m \x1b[1;36m\u\x1b[0m@\x1b[1;34m\h\x1b[0m:\x1b[1;33m\d\x1b[0m → '
prompt_continuation = '… '
less_chatty = True
login_path_as_host = False
auto_vertical_output = False
keyword_casing = upper
enable_pager = False
pager = 'less'
[colors]
completion-menu.completion.current = 'bg:#11111B #B2BEFF'
completion-menu.completion = 'bg:#11111B #F9DFDB'
completion-menu.meta.completion.current = 'bg:#11111B #B2BEFF'
completion-menu.meta.completion = 'bg:#11111B #F9DFDB'
completion-menu.multi-column-meta = 'bg:#11111B #F9DFDB'
scrollbar.arrow = 'bg:#11111B'
scrollbar = 'bg:#11111B'
search = '#ffffff bg:#4444aa'
search.current = '#ffffff bg:#44aa44'
bottom-toolbar = 'bg:#11111B #cdd6f4'
bottom-toolbar.off = 'bg:#11111B #7f849c'
bottom-toolbar.on = 'bg:#11111B #f9e2af'
search-toolbar = 'noinherit bold'
search-toolbar.text = 'nobold'
system-toolbar = 'noinherit bold'
arg-toolbar = 'noinherit bold'
arg-toolbar.text = 'nobold'
bottom-toolbar.transaction.valid = 'bg:#11111B #a6e3a1 bold'
bottom-toolbar.transaction.failed = 'bg:#11111B #f38ba8 bold'
output.header = "#00ff5f bold"
output.odd-row = ""
output.even-row = ""
output.null = "#808080"
[favorite_queries]
[init-commands]
[alias_dsn]
[alias_dsn.init-commands]

